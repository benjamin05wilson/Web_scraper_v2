{
  "competitor_type": "local",
  "country": "Germany",
  "url": "https://www.otto.de/heimtextilien/bettwaesche/",
  "selectors": {
    "Title": [
      "p.find_tile__name.pl_copy100"
    ],
    "Price": [
      "span.find_tile__retailPrice.pl_headline50.find_tile__priceValue.find_tile__priceValue--red",
      "span.find_tile__priceValue.find_tile__priceValue--strikethrough"
    ],
    "URL": [
      "a.find_tile__productLink"
    ],
    "Image": [
      "img.find_tile__productImage.find_tile__productImage--animated"
    ]
  },
  "itemContainer": "article.product",
  "dismiss_actions": [
    {
      "selector": "[role=\"dialog\"] button:has-text(\"OK\")",
      "text": "OK"
    }
  ],
  "pagination": {
    "type": "next_page",
    "selector": "button.js_pagingLink.ts-link.p_btn50--4th",
    "productsPerPage": 0,
    "max_pages": 10
  }
}