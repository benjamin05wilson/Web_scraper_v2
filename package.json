{
  "name": "windows-browser-scraper",
  "version": "1.0.0",
  "description": "Production-grade interactive web scraper with WebRTC streaming",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch src/server/index.ts",
    "client": "vite",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "playwright": "^1.49.0",
    "ws": "^8.18.0",
    "express": "^4.21.0",
    "cors": "^2.8.5",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/express": "^5.0.0",
    "@types/ws": "^8.5.13",
    "@types/uuid": "^10.0.0",
    "@types/cors": "^2.8.17",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "vite": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1"
  }
}
